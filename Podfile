use_frameworks!

target 'XcodeRemote' do

platform :osx, '10.10'

pod 'Stargate', :git => 'https://github.com/contentful-labs/Stargate'
pod 'PeerKit', :git => 'https://github.com/jpsim/PeerKit'

end

target 'XcodeRemotePhone' do

platform :ios, '8.0'

pod 'Stargate', :git => 'https://github.com/contentful-labs/Stargate'
pod 'PeerKit', :git => 'https://github.com/jpsim/PeerKit'

end

target 'XcodeRemote WatchKit Extension' do

platform :ios, '8.0'

pod 'Stargate', :git => 'https://github.com/contentful-labs/Stargate'
pod 'PeerKit', :git => 'https://github.com/jpsim/PeerKit'

end
